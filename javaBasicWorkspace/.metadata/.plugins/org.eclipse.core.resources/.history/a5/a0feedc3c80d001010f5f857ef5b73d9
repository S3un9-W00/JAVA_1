package chap_04;

public class _08_NestedLoop {
	public static void main(String[] args) {
//		이중 반복문
//		*****
//		*****
//		*****
//		*****
//		*****
		
//		1. *****
		for(int i = 0; i < 5; i++) {
			System.out.print("*");
		}
		System.out.println();
//		2.
//		*
//		*
//		*
//		*
//		*
		for(int i = 0; i < 5; i++) {
			System.out.println("*");
		}
	}
}
